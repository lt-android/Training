<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke
                android:width="0.5dp"
                android:color="#0b0b0b"></stroke>
            <solid
                android:color="#f2f2f2"></solid>

            <corners
                android:radius="39dp"></corners>

        </shape>
    </item>
    <item >
        <shape>
            <stroke
                android:width="0.5dp"
                android:color="#D1ECD3"></stroke>
            <solid
                android:color="#faf7f7"></solid>
            <corners
                android:radius="39dp"></corners>
        </shape>
    </item>
</selector>